# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  raycaster_config = debug
endif
ifeq ($(config),release)
  raycaster_config = release
endif

PROJECTS := raycaster

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

raycaster:
ifneq (,$(raycaster_config))
	@echo "==== Building raycaster ($(raycaster_config)) ===="
	@${MAKE} --no-print-directory -C . -f raycaster.make config=$(raycaster_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f raycaster.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   raycaster"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"